{
  "name": "easynvest-gatsby-starter",
  "private": true,
  "description": "Starter para criação de Landing Pages e sites baseado no Gatsby",
  "version": "0.2.0",
  "author": "Roger Ramos <rodgerpaulo@hotmail.com>",
  "dependencies": {
    "gatsby": "^4.25.7",
    "gatsby-image": "^2.0.37",
    "gatsby-plugin-manifest": "^2.0.28",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-prefetch-google-fonts": "^1.4.2",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-preset-react-app": "^7.0.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "lint-staged": "^8.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby",
    "styled-components"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"src/**/*.js\"",
    "lint-staged": "lint-staged",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "lintjs": "eslint -c .eslintrc src/**/*.js",
    "prepush": "npm run lintjs && npm run test:ci"
  },
  "pre-commit": "lint-staged",
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "git add"
    ],
    "src/**/*.jsx": [
      "eslint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodgerpaulo/easynvest-gatsby-starter"
  },
  "bugs": {
    "url": "https://github.com/rodgerpaulo/easynvest-gatsby-starter/issues"
  }
}
